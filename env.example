# Telegram Bot Token
TELEGRAM_TOKEN=seu_token_do_telegram

# API WorkBuscas
API_TOKEN=seu_token_da_api
API_BASE_URL=https://completa.workbuscas.com/api

# WebShare Proxy API Key
USE_PROXY=true
# Valores aceitos: true, false, 1, 0, yes, no
# Se USE_PROXY=false, o bot funcionará sem proxies
WEBSHARE_API_KEY=sua_chave_webshare
WEBSHARE_API_URL=https://proxy.webshare.io/api/v2/proxy/list

# Portainer API Configuration (opcional - apenas se usar deploy-portainer.js localmente)
PORTAINER_URL=sua_url_do_portainer
PORTAINER_USERNAME=seu_usuario_portainer
PORTAINER_PASSWORD=sua_senha_portainer
PORTAINER_ENDPOINT_ID=1
PORTAINER_STACK_NAME=telegram-cpf-bot
PORTAINER_STACK_FILE=docker-compose.yml

# Git Repository Configuration (opcional - apenas se usar deploy-portainer.js)
GIT_REPOSITORY_URL=https://github.com/seu-usuario/seu-repo.git
GIT_REFERENCE=main
COMPOSE_FILE_PATH=docker-compose.yml

# Credenciais Git (opcional - apenas para repositórios privados)
GIT_USERNAME=seu_usuario_github
GIT_PASSWORD=seu_token_github_aqui

